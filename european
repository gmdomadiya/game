                setTimeout(function () {
                  $('.D3Cube1').css('transform-style', 'preserve-3d');
                  $('.D3Cube1').css('-moz-transform-style', 'preserve-3d');
                  $('.D3Cube1').css('-webkit-transform-style', 'preserve-3d');
                  $('.D3Cube1').css('-ms-transform-style', 'preserve-3d');
                  $('.D3Cube1').css('-o-transform-style', 'preserve-3d');
                }, 100);
              
                function closesoft() {
                  window.location = "gameselection.html"
                }
                /********************** BUTTIN (NUMBER) FUNCTION START *******************/
                var first = 1;
                var int;
              
                //*************************Mouse Down Event**************************************
                $(".allpress").mousedown(function (event) {
                  var bet = localStorage.getItem("rou_bet");
                  if (bet == '0') {
                    var betAudio = document.getElementById('betAudio');
                    betAudio.currentTime = 0
                    betAudio.play();
              
                    document.getElementById('betline').innerHTML='';
                    if (event.button == 0) {
              
                      if (first == '1') {
                        num = 2;
                        ok_btn(num);
                        first = 0;
                      }
                      $('.ok_btn').show();
                      $('.pre_btn').hide();
              
                      var dataval = $(this).attr('data-value');
                      int = setInterval(function () { performWhileMouseDown(dataval) }, 100);
                    }
                    else if (event.button == 2) {
                      var no = $(this).attr('data-value');
                      $("#dis_no_" + no).css('display', 'none');
                      var coin = document.getElementById('coin').value;
              
                      document.getElementById('btotal').value = parseFloat(document.getElementById('btotal').value) - parseFloat(document.getElementById('no_' + no).value);
                      document.getElementById('b_total').innerHTML = document.getElementById('btotal').value;
                      var showScore = document.getElementById('showScore').innerHTML.toFixed(2);
                      document.getElementById('showScore').innerHTML = parseFloat(showScore) + parseFloat(document.getElementById('no_' + no).value);
                      document.getElementById('no_' + no).value = 0;
                      if (document.getElementById('no_' + no).value == 0) {
                        document.getElementById('dis_no_' + no).innerHTML = '';
                      }
                      if (document.getElementById('b_total').innerHTML == '0' || document.getElementById('b_total').innerHTML == '') {
                        num = 2;
                        ok_btn(num);
                        first = 1;
                      }
                    }
                  }
                });
                //*************************Mouse Down Event**************************************
                //*************************Mouse Up Event**************************************
                $(".allpress").mouseup(function (event) {
                  clearInterval(int);
                });
                //*************************Mouse Up Event**************************************
                //*************************Mouse Move Event**************************************
                $(".allpress").mousemove(function (event) {
                  clearInterval(int);
                });
                //*************************Mouse Move Event**************************************
                //*************************Mouse Click Event**************************************
                $(".allpress").click(function (btn) {
                  var bet = localStorage.getItem("rou_bet");
                  var clicks = localStorage.getItem('clicks');
                  
                  if (bet == '0') {
                    var betAudio = document.getElementById('betAudio');
                    betAudio.currentTime = 0
                    betAudio.play();
                    
                    document.getElementById('betline').innerHTML='';
                    if (event.button == 0 || clicks == '0') {
                      if (first == '1') {
                        num = 2;
                        ok_btn(num);
                        first = 0;
                      }
                      $('.ok_btn').show();
                      $('.pre_btn').hide();
              
                      var dataval = $(this).attr('data-value');
                      performWhileMouseDown(dataval)
                    }
                    else if (event.button == 2 || clicks == '1') {
                      console.log("right")
              
                      var no = $(this).attr('data-value');
                      $("#dis_no_" + no).css('display', 'none');
                      var coin = document.getElementById('coin').value;
                      document.getElementById('btotal').value = parseFloat(document.getElementById('btotal').value) - parseFloat(document.getElementById('no_' + no).value);
                      document.getElementById('b_total').innerHTML = document.getElementById('btotal').value;
                      var showScore = document.getElementById('showScore').innerHTML.toFixed(2);
                      document.getElementById('showScore').innerHTML = parseFloat(showScore) + parseFloat(document.getElementById('no_' + no).value);
              
                      document.getElementById('no_' + no).value = 0;
                      if (document.getElementById('no_' + no).value == 0) {
                        document.getElementById('dis_no_' + no).innerHTML = '';
                      }
                      if (document.getElementById('b_total').innerHTML == '0' || document.getElementById('b_total').innerHTML == '') {
                        num = 2;
                        ok_btn(num);
                        first = 1;
                      }
                    }
                  }
                });
                //*************************Mouse Click Event**************************************
              
              
                function performWhileMouseDown(no) {
                  var totalBal = document.getElementById('score').innerHTML;
                  var b_total = document.getElementById('b_total').innerHTML;
              
                  var coin = document.getElementById('coin').value;
                  if (b_total == '') { b_total = 0; }

                  var demo = parseFloat(b_total) + parseFloat(coin);
                  if (totalBal >= demo) {
              
                    var limit5000 = document.getElementById('no_' + no).value;
                    var coinLimit = document.getElementById('coin').value;
                    var finalLimit = parseFloat(limit5000) + parseFloat(coinLimit);
              
                    if (finalLimit <= 5000) {
                      $("#dis_no_" + no).css('display', 'block');
                      var coin = document.getElementById('coin').value
                      document.getElementById('no_' + no).value = parseFloat(document.getElementById('no_' + no).value) + parseFloat(coin);
                      document.getElementById('dis_no_' + no).innerHTML = document.getElementById('no_' + no).value;
              
                      document.getElementById('btotal').value = parseFloat(document.getElementById('btotal').value) + parseFloat(coin);
                      document.getElementById('b_total').innerHTML = document.getElementById('btotal').value;

              
                      document.getElementById('showScore').innerHTML = parseFloat(totalBal) - parseFloat(document.getElementById('b_total').innerHTML);
                    }
                  }
                }
              
                /********************** BUTTIN (NUMBER) FUNCTION END *******************/
                $("img").mousedown(function () {
                  return false;
                });



