    if (event.button == 0) {
     localStorage.setItem('clicks','0');
    } else {
      localStorage.setItem('clicks','1');
    }
    if (no == 14) {
      $('.allpress[data-value=' + no + ']').click();
      $('.allpress[data-value=1]').click();
      $('.allpress[data-value=20]').click();
      $('.allpress[data-value=31]').click();
      $('.allpress[data-value=9]').click();
    }
  }
