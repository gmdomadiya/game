$(document).ready(function () {
  $("#coin_0,#coin_1,#coin_2,#coin_3,#coin_4,#coin_5,#coin_6,#coin_7,#coin_8,#coin_9,#coin_10,#coin_11,#coin_12,#coin_13,#coin_14,#coin_15,#coin_16,#coin_17,#coin_18,#coin_19,#coin_20,#coin_21,#coin_22,#coin_23,#coin_24,#coin_25,#coin_26,#coin_27,#coin_28,#coin_29,#coin_30,#coin_31,#coin_32,#coin_33,#coin_34,#coin_35,#coin_36").mouseenter(function () {
     $("#toolt").show();
  });
  $("#coin_0,#coin_1,#coin_2,#coin_3,#coin_4,#coin_5,#coin_6,#coin_7,#coin_8,#coin_9,#coin_10,#coin_11,#coin_12,#coin_13,#coin_14,#coin_15,#coin_16,#coin_17,#coin_18,#coin_19,#coin_20,#coin_21,#coin_22,#coin_23,#coin_24,#coin_25,#coin_26,#coin_27,#coin_28,#coin_29,#coin_30,#coin_31,#coin_32,#coin_33,#coin_34,#coin_35,#coin_36").mouseleave(function () {
     $("#toolt").hide();
  });
  $("#coin_0,#coin_1,#coin_2,#coin_3,#coin_4,#coin_5,#coin_6,#coin_7,#coin_8,#coin_9,#coin_10,#coin_11,#coin_12,#coin_13,#coin_14,#coin_15,#coin_16,#coin_17,#coin_18,#coin_19,#coin_20,#coin_21,#coin_22,#coin_23,#coin_24,#coin_25,#coin_26,#coin_27,#coin_28,#coin_29,#coin_30,#coin_31,#coin_32,#coin_33,#coin_34,#coin_35,#coin_36").click(function () {
     $("#toolt").hide();
  })
});


$(document).ready(function () {
  $("#coin_2to1,#coin_2to1_2,#coin_2to1_3,#coin_1st12,#coin_2nd12,#coin_3rd12").mouseenter(function () {
     $("#toolt1").show();
  });
  $("#coin_2to1,#coin_2to1_2,#coin_2to1_3,#coin_1st12,#coin_2nd12,#coin_3rd12").mouseleave(function () {
     $("#toolt1").hide();
  });
  $("#coin_2to1,#coin_2to1_2,#coin_2to1_3,#coin_1st12,#coin_2nd12,#coin_3rd12").click(function () {
     $("#toolt1").hide();
  })
});

$(document).ready(function () {
  $("#coin_2to1,#coin_1t18,#coin_19t36,#coin_red,#coin_black,#coin_odd,#coin_even").mouseenter(function () {
     $("#toolt2").show();
  });
  $("#coin_2to1,#coin_1t18,#coin_19t36,#coin_red,#coin_black,#coin_odd,#coin_even").mouseleave(function () {
     $("#toolt2").hide();
  });
  $("#coin_2to1,#coin_1t18,#coin_19t36,#coin_red,#coin_black,#coin_odd,#coin_even").click(function () {
     $("#toolt2").hide();
  })
});

  function track23(no) {
    if (event.button == 0) {
     localStorage.setItem('clicks','0');
    } else {
      localStorage.setItem('clicks','1');
    }
    if (no == 23) {
      $('.allpress[data-value=' + no + ']').click();
      $('.allpress[data-value=8]').click();
      $('.allpress[data-value=10]').click();
      $('.allpress[data-value=30]').click();
      $('.allpress[data-value=5]').click();
    }
  }
