$('.allpress').hover(function(){
  var value = $(this).data('value');

  var totalbet= document.getElementById('dis_no_'+value+'').innerHTML;
  var pays= totalbet*35;
  $('#coin_'+value).prop('title', 'Bet: '+totalbet+' \nPays: *35 \nReturns: '+pays+' \nMin Bet: 1 \nMax Bet: 5000');
});

$('.combine').hover(function(){
  var value = $(this).data('value');

  var totalbet= document.getElementById('dis_no_'+value+'').innerHTML;
  var pays= totalbet*17.50;
  $('#coin_'+value).prop('title', 'Bet: '+totalbet+' \nPays: *17.50 \nReturns: '+pays+' \nMin Bet: 1 \nMax Bet: 5000');
});

